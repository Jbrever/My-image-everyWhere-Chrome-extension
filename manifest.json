{
    "manifest_version":2,
    "name":"my pic everywhere",
    "version":"0.0.1",
    "web_accessible_resources":[
        "myimages/*.jpeg"
    ],
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["content.js"]
        }
    ],
    "background":{
        "scripts":["background.js"]
    },
    "browser_action":{
        "default_icon":"img.png"
    },
    "icons":{
        "500":"img.png"
    }
}